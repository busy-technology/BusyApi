{
  "definitions": {},
  "type": "object",
  "title": "Send Message Request Schema",
  "required": ["symbol", "tokenType"],
  "additionalProperties": false,
  "properties": {
    "symbol": {
      "type": "string",
      "title": "User ID",
      "pattern": "^(?=.{3,5}$)(?=.*[a-zA-Z])[a-zA-Z0-9]+([-]{0,1}[a-zA-Z0-9]+)$"
    },
    "tokenType": {
      "type": "string",
      "title": "token type",
      "enum": ["BUSY", "BUSY20", "NFT", "GAME"]
    }
  }
}
