{
  "definitions": {},
  "type": "object",
  "additionalProperties": false,
  "title": "Authorization schema",
  "required": ["userId", "password"],
  "properties": {
    "userId": {
      "type": "string",
      "title": "user Id",
      "pattern": "^[a-zA-Z0-9!@#+=._-]+$"
    },
    "password": {
      "type": "string",
      "title": "password",
      "pattern": "^\\$2[ayb]\\$.{56}$"
    }
  }
}
