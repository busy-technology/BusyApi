{
  "definitions": {},
  "type": "object",
  "title": "Query Wallet Schema",
  "additionalProperties": false,
  "required": ["walletId", "token"],
  "properties": {
    "walletId": {
      "type": "string",
      "title": "Wallet ID",
      "pattern": "^B-[a-zA-Z0-9]+$"
    },
    "token": {
      "type": "string",
      "title": "Symbol",
      "pattern": "^(?=.{3,5}$)(?=.*[a-zA-Z])[a-zA-Z0-9]+([-]{0,1}[a-zA-Z0-9]+)$"
    }
  }
}
